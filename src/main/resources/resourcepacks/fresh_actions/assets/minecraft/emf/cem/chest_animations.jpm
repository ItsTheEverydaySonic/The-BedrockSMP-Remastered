{
	"credit": "Created by FreshLX for Fresh Animations",
	"animations": [
		{
			"varb.fcc": "frame_counter == var.frame_counter_prev",
			"var.hover": "if( is_hovered && var.hover<2.5, var.hover +frame_time, 0 )*0",
			"var.lid_new": "clamp( if( varb.fcc, var.lid_new, var.hover>1 || lid.rx!=0 && var.pre_lid>=lid.rx, var.lid_new +1.5 *frame_time, var.lid_new -2 *frame_time ), 0, 1 )",
			"var.lid": "-lid.rx/pi*2*0 +1-pow( 1-var.lid_new, 2 )",
			"var.close": "clamp( if( varb.fcc, var.close, var.pre_lid_new>var.lid || var.lid==1, var.close +9 *frame_time, var.close -9 *frame_time ), 0, 1 )"
		},
		{
			"base.rx": "pi",
			"base.tx": "8.01",
			"base.tz": "8",
			"back_pivot.rx": " torad( if(varb.unique,-4,-2)*max(0, sin( min(1, var.lid_new*4 -0.6 )*pi ) )*(1-var.close)      )",
			"front_pivot.rx": "torad( if(varb.unique,     2*max(0, sin( min(1, var.lid_new*4      )*pi ) )*(1-var.close), 0 ) )",
			"back_pivot.ty": "max( 0, if(varb.unique,              sin( min(1, var.lid_new*5 -0.3 )*pi )  *(1-var.close), 0 ) )",
			"lid2.rx": " torad( -82*( max(0,      var.lid*1.3          -0.3 )*var.close  +(1-cos( max(0, (var.lid+sin(var.lid*pi/1.2))/1.5 -0.4*(1-var.lid) )*pi/2 ) )*(1-var.close) )  ) -back_pivot.rx",
			"latch.rx": "torad( -20*( max(0, sin( var.lid*1.7*pi )*2.2 -0.7 )*var.close  +sin( min(1, var.lid*3 )*pi )/1.5*(1-var.close) )  )",
			"latch.ty": "2 +min(1, var.lid*4 )  -sin( min(1, var.lid*3 )*pi )*var.close",
			"button.tz": "-1 +min(1, var.lid*6 -0.4 )*var.close  +min(1, var.lid*2 )*(1-var.close)"
		},
		{
			"var.pre_lid": "lid.rx",
			"var.pre_lid_new": "var.lid",
			"var.frame_counter_prev": "frame_counter"
		}
	]
}